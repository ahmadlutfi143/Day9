            <div class="row row-cols-1 row-cols-md-3 g-4 mb-5">
               {{#each addProject}}
                <div class="col">
                  <div class="card h-100">
                    <a href="/project-detail/{{this.id}}" target="_blank"><img src="../public/tech6.jpg" class="card-img-top" alt="..."></a>
                    <div class="card-body">
                      <h5 class="card-title">{{this.name}}</a></h5>
                      <p>durasi : {{this.duration}}</p>
                      <p class="card-text-content">{{this.description}}</p>
                      <div class="logo">
                        {{#if this.technologies.[0]}}<i class="fa-brands fa-html5"></i>{{/if}}
                        {{#if this.technologies.[1]}}<i class="fa-brands fa-css3-alt"></i>{{/if}}
                        {{#if this.technologies.[2]}}<i class="fa-brands fa-react"></i>{{/if}}
                        {{#if this.technologies.[3]}}<i class="fa-brands fa-js-square"></i>{{/if}}
                        
                        
                        </div>
                    </div>
                    <div class="btn-project">
                        <button type="button" class="btn btn-dark"><a href="/project/edit/{{this.id}}">edit</a></button>
                        <button type="button" class="btn btn-dark"><a href="/project/delete/{{this.id}}">delete</a></button>
                    </div>
                  </div>
                </div>
